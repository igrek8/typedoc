{
  "name": "@igrek8/typedoc",
  "version": "0.0.0",
  "license": "MIT",
  "files": [
    "dist",
    "bin",
    "typedoc.js",
    "public/index.html"
  ],
  "bin": {
    "typedoc": "bin/typedoc"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "@igrek8/eslint-config"
    ],
    "env": {
      "node": true
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release": {
    "extends": [
      "@igrek8/semantic-release-config"
    ]
  },
  "prettier": "@igrek8/prettier-config",
  "scripts": {
    "lint": "eslint \"**/*\"",
    "build": "tsc -p tsconfig.build.json",
    "format": "prettier --check \"**/*\""
  },
  "peerDependencies": {
    "commander": "^10",
    "gh-pages": "^5",
    "typedoc": "^0",
    "typedoc-plugin-markdown": "^3"
  },
  "devDependencies": {
    "@commitlint/cli": "17.8.1",
    "@commitlint/config-conventional": "17.8.1",
    "@igrek8/eslint-config": "1.0.6",
    "@igrek8/prettier-config": "1.0.11",
    "@igrek8/semantic-release-config": "1.0.11",
    "@igrek8/tsconfig": "1.0.11",
    "commander": "11.1.0",
    "typedoc": "0.25.7",
    "typedoc-plugin-markdown": "3.17.1",
    "gh-pages": "6.1.1",
    "husky": "9.0.9"
  }
}
